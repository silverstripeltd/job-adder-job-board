{
    "name": "biffbangpow/job-adder-job-board",
    "description": "A basic job board for SilverStripe",
    "keywords": ["silverstripe"],
    "type": "silverstripe-vendormodule",
    "license": "proprietary",
    "authors": [
        {"name": "James Heathcote","email": "james.h@biffbangpow.com"}
    ],
    "require": {
        "silverstripe/cms": "^5",
        "silverstripe/framework": "^5",
        "silverstripe/lumberjack": "^3",
        "silverstripe/admin": "^2",
        "guzzlehttp/guzzle": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "BiffBangPow\\JobAdderJobBoard\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "silverstripe/vendor-plugin": true
        }
    }
}
